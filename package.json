{
  "name": "@eox/elements",
  "private": true,
  "version": "2.0.0",
  "workspaces": [
    "elements/*"
  ],
  "scripts": {
    "build:all": "npm run build --ws",
    "clean": "npm exec --workspaces -- npx rimraf node_modules && npx rimraf node_modules",
    "cypress": "cypress open",
    "test:all": "cypress run --component",
    "format": "prettier --write .",
    "format:all": "npm run format --ws",
    "format:check:all": "prettier --check .",
    "lint:all": "npm run lint --ws",
    "lint:fix:all": "npm run lint:fix --ws",
    "watch:all": "npm run watch --ws",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-manifest": "npx custom-elements-manifest analyze --globs './elements/itemfilter/src/main.ts' './elements/map/main.ts' './elements/chart/src/main.ts' './elements/drawtools/src/main.ts' './elements/layercontrol/src/main.ts' './elements/stacinfo/src/main.ts' './elements/timecontrol/src/main.ts' --litelement --watch"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.8.4",
    "@cypress/mount-utils": "^4.0.0",
    "@storybook/addon-docs": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/web-components": "^7.4.0",
    "@storybook/web-components-vite": "^7.4.0",
    "custom-elements-manifest": "^2.0.0",
    "cypress": "^12.12.0",
    "cypress-lit": "^0.0.8",
    "rimraf": "^5.0.1",
    "storybook": "^7.4.0"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "dependencies": {
    "eslint-plugin-cypress": "^2.14.0"
  }
}
