{
  "name": "@eox/layout",
  "version": "0.3.0",
  "type": "module",
  "devDependencies": {
    "vite": "^6.0.3"
  },
  "files": [
    "types",
    "dist",
    "src"
  ],
  "main": "./src/main.js",
  "types": "./types/src/main.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./types/main.d.ts"
      ],
      "src/*": [
        "./types/*"
      ]
    }
  },
  "scripts": {
    "types:generate": "tsc --project tsconfig.build.json || true",
    "prepack": "npm run types:generate",
    "build": "vite build",
    "watch": "vite build --watch"
  }
}
