{
  "name": "@eox/jsonform",
  "version": "1.1.1",
  "type": "module",
  "devDependencies": {
    "@eox/map": "*",
    "vite": "^6.0.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "files": [
    "types",
    "dist",
    "src"
  ],
  "main": "./src/main.js",
  "types": "./types/jsonform/src/main.d.ts",
  "typesVersions": {
    "*": {
