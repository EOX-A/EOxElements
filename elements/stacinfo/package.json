{
  "name": "@eox/stacinfo",
  "version": "0.6.2",
  "type": "module",
  "devDependencies": {
    "vite": "^6.0.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "files": [
    "types",
    "dist",
    "src"
  ],
  "main": "./src/main.js",
  "types": "./types/main.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./types/main.d.ts"
      ],
      "src/*": [
        "./types/*"
      ]
    }
  },
  "scripts": {
    "types:generate": "tsc --project tsconfig.build.json || true",
    "prepack": "npm run types:generate",
    "build": "vite build",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@eox/ui": "^0.3.2",
    "@radiantearth/stac-fields": "^1.5.4",
    "lit": "^3.2.0"
  }
}
