{
  "name": "@eox/drawtools",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@eox/map": "*",
    "vite": "^6.0.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "files": [
    "types",
    "dist",
    "src"
  ],
  "main": "./src/main.js",
  "types": "./types/drawtools/src/main.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./types/drawtools/src/main.d.ts"
      ],
      "src/*": [
        "./types/drawtools/src/*"
      ]
    }
  },
  "scripts": {
    "types:generate": "tsc --project tsconfig.build.json || true",
    "prepack": "npm run types:generate",
    "build": "vite build",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@eox/elements-utils": "^1.0.0",
    "@eox/ui": "^0.3.6",
    "lit": "^3.2.0",
    "ol": "^10.4.0"
  }
}
