{
  "name": "@eox/timeslider",
  "version": "0.0.1",
  "type": "module",
  "devDependencies": {
    "ol": "^10.4.0",
    "vite": "^6.0.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./src/main.js",
  "types": "./types/timeslider/src/main.d.ts",
